<div class="name-side">
  <div class="user-name">
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class="chat-link">
    <%= link_to "チャットを作成する", new_room_path %>
  </div>
</div>

<div class="under-side">
  <% current_user.rooms.each do |room| %>
    <div class="rooms">
      <div class="chat-room">
        <a href="#"><%= room.name %></a>
      </div>
    </div>
  <% end %>
</div>